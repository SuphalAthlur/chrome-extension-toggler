{
  "manifest_version": 3,
  "name": "Extension Toggler",
  "description": "This chrome extension disables/enables other chrome extensions.",
  "version": "1.0",
  "action": {
    "default_icon": {
      "16": "icons/extensions-page-16.png",
      "32": "icons/extensions-page-32.png"
    }
  },
  "permissions": [
    "management"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "icons": {
    "48": "icons/extensions-page-48.png",
    "128": "icons/extensions-page-128.png"
  }
}
